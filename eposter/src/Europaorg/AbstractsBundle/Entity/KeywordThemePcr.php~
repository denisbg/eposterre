<?php

namespace AbstractBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * KeywordThemePcr
 *
 * @ORM\Table(name="keyword-theme-pcr")
 * @ORM\Entity
 */
class KeywordThemePcr
{
    /**
     * @var integer
     *
     * @ORM\Column(name="idTheme", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $idtheme;

    /**
     * @var string
     *
     * @ORM\Column(name="themeData", type="string", length=255, nullable=false)
     */
    private $themedata;

    /**
     * @var integer
     *
     * @ORM\Column(name="themeOrder", type="integer", nullable=false)
     */
    private $themeorder;

    /**
     * @var boolean
     *
     * @ORM\Column(name="deleted", type="boolean", nullable=false)
     */
    private $deleted = '0';


}

